{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "esModuleInterop": true,
    // 下面是设置装饰器可运行的代码
    "experimentalDecorators":true,
    "sourceMap": true,
    "baseUrl": ".",
    "strict": true,
    "paths": {
      "@/*": ["src/*"]
    },
    "allowSyntheticDefaultImports": true,
    //添加下面的配置忽略类型默认设置(起因是dispatch类型)
    // "noImplicitAny":false
  }
}
